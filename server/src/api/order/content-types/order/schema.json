{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "fullname": {
      "type": "string"
    },
    "email": {
      "type": "email",
      "required": true
    },
    "order_status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "paid",
        "cancelled"
      ]
    },
    "plans": {
      "type": "enumeration",
      "enum": [
        "bronze",
        "silver",
        "gold"
      ]
    },
    "payment_method": {
      "type": "enumeration",
      "enum": [
        "mobile money",
        "bank transfer"
      ]
    }
  }
}
