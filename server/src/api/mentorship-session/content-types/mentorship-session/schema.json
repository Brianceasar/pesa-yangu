{
  "kind": "collectionType",
  "collectionName": "mentorship_sessions",
  "info": {
    "singularName": "mentorship-session",
    "pluralName": "mentorship-sessions",
    "displayName": "Mentorship Session"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "text",
      "required": true
    },
    "session_status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "approved",
        "completed",
        "rejected"
      ]
    },
    "preferred_date": {
      "type": "datetime"
    },
    "duration": {
      "type": "integer"
    },
    "customer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "mentorship_sessions"
    },
    "rating": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::rating.rating",
      "mappedBy": "session"
    },
    "mentor": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "mentoring_sessions"
    },
    "description": {
      "type": "string",
      "required": true
    }
  }
}
